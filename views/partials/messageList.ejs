<% if(locals.messages.length > 0) {%>
    <% messages.forEach((message) => {%>
        <div>
            <%=message.title%>
            <%=message.content%>
            Date: <%=message.created_at%>
            Author: <%=message.firstname%> <%=message.lastname%>
        </div>
    <%})%>
<%} else {%>
    <p>There are no messages</p>
    <%}%>